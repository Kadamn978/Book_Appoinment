            <td>
                @Html.DisplayFor(modelItem => item.D_Specialization)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.D_Experience)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.D_Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Confirm_D_Password)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.D_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.D_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.D_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
