            <th>
                @Html.DisplayNameFor(model => model.D_Specialization)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.D_Experience)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.D_Password)
