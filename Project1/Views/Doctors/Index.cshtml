                @Html.DisplayNameFor(model => model.D_FName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.D_LName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.D_ContactNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.D_EMail)
